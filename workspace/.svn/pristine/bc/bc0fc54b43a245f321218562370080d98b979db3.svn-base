<!DOCTYPE html>
<html lang="en">

<head>

<!--  Start - Should be present on top of head on all pages -->
<script src="../bower_components/jquery/jquery-latest.min.js"
	type="text/javascript"></script>

<!-- JQuery cookie manager - https://github.com/js-cookie/js-cookie -->
<script src="../js/cookieMonster.js"></script>

<!-- Main js used to inject content used throughout the site, and manage session vars -->
<script src="../js/main.js" type="text/javascript"></script>

<!--  End - Should be present on top of head on all pages -->

<!-- Font Awesome Plugin -->
<link href="../css/font-awesome.css" rel="stylesheet" type="text/css">

<!-- jQuery -->
<script src="../bower_components/jquery/dist/jquery.min.js"></script>

<!-- Register page javascript -->
<script src="../js/register.js" type="text/javascript"></script>

<!-- Custom (Formerly Injected) -->
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1">
<meta name="description" content="">
<meta name="author" content="">

<title>Turing Point - Clicker Replacement</title>

<!-- Bootstrap Core CSS -->
<link href="../bower_components/bootstrap/dist/css/bootstrap.min.css"
	rel="stylesheet">

<!-- MetisMenu CSS -->
<link href="../bower_components/metisMenu/dist/metisMenu.min.css"
	rel="stylesheet">

<!-- Timeline CSS -->
<link href="../dist/css/timeline.css" rel="stylesheet">

<!-- Custom CSS -->
<link href="../dist/css/sb-admin-2.css" rel="stylesheet">

<!-- Custom Fonts -->
<link href="../bower_components/font-awesome/css/font-awesome.min.css"
	rel="stylesheet" type="text/css">

<!-- Our CSS -->
<link href="../css/style.css" rel="stylesheet" type="text/css">

<!-- Bootstrap Core JavaScript -->
<script src="../bower_components/bootstrap/dist/js/bootstrap.min.js"></script>

<!-- Metis Menu Plugin JavaScript -->
<script src="../bower_components/metisMenu/dist/metisMenu.min.js"></script>

<!-- Custom Theme JavaScript -->
<script src="../dist/js/sb-admin-2.js"></script>

<!-- Favorite Icons -->
<link rel="icon" type="image/png" href="../img/favicon-32x32.png"
	sizes="32x32" />
<link rel="icon" type="image/png" href="../img/favicon-16x16.png"
	sizes="16x16" />


</head>

<body>

	<!-- Header injected here -->
	<div id="header"></div>

	<div class="container">


		<br> <br>
		<div class="row">
			<div class="col-md-6 col-md-offset-3">
				<div class="panel panel-default">
					<div class="panel-heading">
						<h3 class="panel-title">Account Registration</h3>
					</div>
					<div class="panel-body">
						<form role="form" class="registrationForm">

							<div class="row">
								<div class="col-md-6">
									<div class="form-group">
										<label for="fName">First Name:</label> <input type="text"
											class="form-control" name="fName" placeholder="First"
											autofocus>
									</div>
									<div id="fNameError"></div>
								</div>
								<div class="col-md-6">
									<div class="form-group">
										<label for="fName">Last Name:</label> <input type="text"
											class="form-control" name="lName" placeholder="Last">
									</div>
									<div id="lNameError"></div>
								</div>

							</div>

							<div class="form-group">
								<label for="email">Email:</label> <input type="text"
									class="form-control" name="email"
									placeholder="example@utah.edu">
							</div>
							<div id="emailError"></div>

							<div class="form-group">
								<label for="password">Password:</label> <input type="password"
									class="form-control" name="password" id="password"
									placeholder="Password">
							</div>
							<div id="passwordError"></div>

							<div class="form-group">
								<label for="passwordConfirm">Confirm Password:</label> <input
									type="password" class="form-control" name="passwordConfirm"
									id="passwordConfirm" placeholder="Password">
							</div>
							<div id="passwordConfError"></div>
							<div id="passwordMatchError"></div>

							<label for="role">Role:</label>
							<div class="form-group">
								<div class="form-group">
									<ul class="nav nav-pills" id="rolePills">
										<li class="active" role="STUDENT"><a data-toggle="pill"
											href="#student">Student</a></li>
										<li role="INSTRUCTOR"><a data-toggle="pill"
											href="#instructor">Instructor</a></li>
									</ul>
								</div>

								<div class="form-group">
									<div class="tab-content">
										<div id="student" class="tab-pane fade in active">
											<label for="studID">Student ID:</label> <input type="text"
												class="form-control" name="studID" placeholder="u0123456">
											<div id="idError"></div>
										</div>
										<div id="instructor" class="tab-pane fade">
											<label for="accessCode">Access Code:</label> <input
												type="text" class="form-control" name="accessCode"
												id="accessCode" placeholder="Ex: ACG451">
											<div id="accessError"></div>
										</div>
									</div>
								</div>
							</div>

							<div id="registrationError"></div>

							<p>
								<button class="btn btn-lg btn-primary btn-block" type="submit" value="Submit" id="registerBtn">
								<span
									class="icon-refresh icon-spin hidden" id="registrationSpinner"></span> Submit Registration
								</button>
							</p>

						</form>
					</div>
				</div>
			</div>
		</div>
	</div>

</body>

</html>
