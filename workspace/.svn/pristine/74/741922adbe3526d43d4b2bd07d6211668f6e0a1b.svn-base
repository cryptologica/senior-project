	type="text/javascript"></script>

<!-- JQuery cookie manager - https://github.com/js-cookie/js-cookie -->
<script src="../js/cookieMonster.js"></script>

<!-- Main js used to inject content used throughout the site, and manage session vars -->
<script src="../js/main.js" type="text/javascript"></script>

<!-- Favorite Icons -->
<link rel="icon" type="image/png" href="../img/favicon-32x32.png"
	sizes="32x32" />
<link rel="icon" type="image/png" href="../img/favicon-16x16.png"
	sizes="16x16" />

<!--  End - Should be present on top of head on all pages -->



<!-- Custom (Formerly Injected) -->
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1">
<meta name="description" content="">
<meta name="author" content="">

<title>Turing Point - Clicker Replacement</title>

<!-- Bootstrap Core CSS -->
<link href="../bower_components/bootstrap/dist/css/bootstrap.min.css"
	rel="stylesheet">

<!-- MetisMenu CSS -->
<link href="../bower_components/metisMenu/dist/metisMenu.min.css"
	rel="stylesheet">

<!-- Timeline CSS -->
<link href="../dist/css/timeline.css" rel="stylesheet">

<!-- Custom CSS -->
<link href="../dist/css/sb-admin-2.css" rel="stylesheet">

<!-- DataTables CSS -->
<link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/t/bs/dt-1.10.11,r-2.0.2/datatables.min.css"/>

<!-- Custom Fonts -->
<link href="../bower_components/font-awesome/css/font-awesome.min.css"
	rel="stylesheet" type="text/css">

<!-- Our CSS -->
<link href="../css/style.css" rel="stylesheet" type="text/css">

<!-- Bootstrap Core JavaScript -->
<script src="../bower_components/bootstrap/dist/js/bootstrap.min.js"></script>

<!-- Metis Menu Plugin JavaScript -->
<script src="../bower_components/metisMenu/dist/metisMenu.min.js"></script>

<!-- Custom Theme JavaScript -->
<script src="../dist/js/sb-admin-2.js"></script>

<!-- DataTable JS -->
<script type="text/javascript" src="https://cdn.datatables.net/t/bs/dt-1.10.11,r-2.0.2/datatables.min.js"></script>

<!-- Add Class Page JS -->
<script type="text/javascript" src="../js/addClass_student.js"></script>

</head>

<body>
	<div id="header"></div>

	<div class="container-fluid">
		<br> <a href="#" data-toggle="tooltip" data-placement="bottom"
			title="Search by Instructor Name, Course Name or Course Description. Click the blue plus icon to view more info or add a course.">
			<label>Help </label> <span class="glyphicon glyphicon-question-sign"></span>
		</a>

		<div class="row">
			<br>
			<div class="col-md-4" id="formError"></div>
		</div>

		<!-- Course Search Table / Spinner -->
		<div class="row">
			<div class="panel-group" style="margin: 10px">
				<div class="panel panel-default">
					<div class="panel-heading">
						<i class="fa fa-spinner fa-spin fa-5x" id="loadClassListSpinner"></i>
						<table id="searchTable" class="table table-striped table-bordered dt-responsive nowrap" cellspacing="0" width="100%">
							<thead>
								<tr>
									<th>Course</th>
									<th>Instructor:</th>
									<th>Description:</th>
									<th></th>
								</tr>
							</thead>
							<tbody id="searchTableData">
							</tbody>
						</table>
					</div>
				</div>
			</div>
		</div>

		<!-- Modal: Successfully Added Course Prompt -->
		<div id="addCourseModal" class="modal fade" role="dialog">
			<div class="modal-dialog">

				<!-- Modal content-->
				<div class="modal-content">
					<div class="modal-header">
						<h4 class="modal-title">Successfully Added Course</h4>
					</div>
					<div class="modal-body">
						<p>Would you like to add another course?</p>
					</div>
					<div class="modal-footer">
						<button type="button" class="btn btn-default" data-dismiss="modal">Yes</button>
						<button type="button" class="btn btn-default" id="quitAddCourseBtn">No</button>
					</div>
				</div>

			</div>
		</div>

	</div>
</html>